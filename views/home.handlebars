<div id="wrapper">
	<h1>Articles</h1>
	<button id="scrapeBtn">Check CS Blog</button>
	<div id="headlines">
		{{#each headline}}
			<div class="headline" articleId="{{_id}}">
				<h2>{{title}}</h2>
				<p>{{summary}}</p>
				<a href="{{link}}" target="_blank" rel="noopener">View Article</a>
			</div>
		{{/each}}
	</div>
	<button id="clearBtn">CLEAR ALL</button>
</div>